package it.uniroma1.di.simulejos.opengl;

import java.io.IOException;
import java.io.Reader;

import javax.media.opengl.GL2GL3;

public class FragmentShader extends Shader {
	public FragmentShader(GL2GL3 gl, Type type) {
		super(gl, type);
	}

	public FragmentShader(GL2GL3 gl, Type type, String source) {
		super(gl, type, source);
	}

	public FragmentShader(GL2GL3 gl, Type type, Reader source)
			throws IOException {
		super(gl, type, source);
	}
}
